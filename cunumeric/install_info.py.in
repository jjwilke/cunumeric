def get_libpath():
    import os, sys
    join = os.path.join
    exists = os.path.exists
    dirname = os.path.dirname
    cn_path = dirname(dirname(__file__))
    if exists(libdir := join(cn_path, "build", "lib")):
        return libdir
    if exists(libdir := join(dirname(dirname(sys.executable)), "lib")):
        return libdir
    return ""


libpath: str = get_libpath()
header: str = """@header@"""
